/**
 */
var App = App || {};
App.Views.PropertyView = Backbone.View.extend({
    template: null,
    tagName: 'li',
    initialize: function() {
        this.template = _.template($('#property-template').html());
    },
    render: function() {
        var html = this.template(this.model.toJSON());
        this.$el.html(html);
        return this;
    }
});
