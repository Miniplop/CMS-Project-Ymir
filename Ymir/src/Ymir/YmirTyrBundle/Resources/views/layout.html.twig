{% extends "::base.html.twig" %} 
{% block stylesheets %}
    {{ parent() }}
	{% stylesheets filter="cssrewrite" output="css/ymir.css"
		"bundles/tyr/css/header.css"
		"bundles/tyr/css/util.css"
		"bundles/tyr/css/accueil.css"
		"bundles/tyr/css/profil.css"
		"bundles/tyr/css/creative.css"
		"bundles/tyr/css/stage.css"
		"bundles/tyr/css/footer.css" %}
		<link rel="stylesheet" href="{{ asset_url }}"/>
	{% endstylesheets %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {% javascripts  output='js/ymir.js'
				"bundles/tyr/js/app.js"
                "bundles/tyr/js/views/projects/*.js"
                "bundles/tyr/js/views/CategorieListView.js"
                "bundles/tyr/js/views/CategorieView.js"
                "bundles/tyr/js/views/users/users_index.js"
                "bundles/tyr/js/models/widget.js"
                "bundles/tyr/js/collections/widgets.js"
                "bundles/tyr/js/models/categorie.js"
                "bundles/tyr/js/collections/categories.js"
                "bundles/tyr/js/models/page.js"
                "bundles/tyr/js/models/project.js"
                "bundles/tyr/js/models/user.js"
                "bundles/tyr/js/collections/projects.js"
                "bundles/tyr/js/collections/users.js"
		%}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script>
            window.onload = function() {
                app.init();
            }
    </script>
{% endblock %}