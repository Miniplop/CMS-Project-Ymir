{% extends "::base.html.twig" %} 
{% block stylesheets %}
    {{ parent() }}
	{% stylesheets filter="cssrewrite" output="css/ymir.css"
		"bundles/tyr/css/header.css"
		"bundles/tyr/css/util.css"
		"bundles/tyr/css/accueil.css"
		"bundles/tyr/css/profil.css"
		"bundles/tyr/css/creative.css"
		"bundles/tyr/css/stage.css"
		"bundles/tyr/css/footer.css"
		"bundles/tyr/css/mockups/device-mockups.css" %}
		<link rel="stylesheet" href="{{ asset_url }}"/>
	{% endstylesheets %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {% javascripts  output='js/ymir.js'
				"bundles/tyr/js/App.js"
				"bundles/tyr/js/DragDropHandler.js"
                "bundles/tyr/js/routers/*.js"
                "bundles/tyr/js/views/projects/ProjectPageView.js"
                "bundles/tyr/js/views/projects/ProjectListView.js"
                "bundles/tyr/js/views/widgets/WidgetView.js"
                "bundles/tyr/js/views/widgets/WidgetListView.js"
                "bundles/tyr/js/views/*.js"
                "bundles/tyr/js/models/*.js"
                "bundles/tyr/js/forms/*.js"
                "bundles/tyr/js/collections/*.js"
		%}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script>
            window.onload = function() {
                app.init();
                $(document).foundation();
                DragDropHandler();
            }
    </script>

{% endblock %}